:root {
    /* The main accent color and the matching text value */
    --adw-accent-bg-rgb: {{base0F-rgb-r}}, {{base0F-rgb-g}}, {{base0F-rgb-b}} !important;
    --adw-accent-fg-rgb: {{base00-rgb-r}}, {{base00-rgb-g}}, {{base00-rgb-b}} !important;
    --adw-accent-rgb: {{base0F-rgb-r}}, {{base0F-rgb-g}}, {{base0F-rgb-b}} !important;

    /* destructive-action buttons */
    --adw-destructive-bg-rgb: {{base08-rgb-r}}, {{base08-rgb-g}}, {{base08-rgb-b}} !important;
    --adw-destructive-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;
    --adw-destructive-rgb: {{base08-rgb-r}}, {{base08-rgb-g}}, {{base08-rgb-b}} !important;

    /* Levelbars, entries, labels and infobars. These don't need text colors */
    --adw-success-bg-rgb: {{base0B-rgb-r}}, {{base0B-rgb-g}}, {{base0B-rgb-b}} !important;
    --adw-success-fg-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-success-rgb: {{base0B-rgb-r}}, {{base0B-rgb-g}}, {{base0B-rgb-b}} !important;

    --adw-warning-bg-rgb: {{base0A-rgb-r}}, {{base0A-rgb-g}}, {{base0A-rgb-b}} !important;
    --adw-warning-fg-rgb: {{base00-rgb-r}}, {{base00-rgb-g}}, {{base00-rgb-b}} !important;
    --adw-warning-fg-a: 0.8 !important;
    --adw-warning-rgb: {{base0A-rgb-r}}, {{base0A-rgb-g}}, {{base0A-rgb-b}} !important;

    --adw-error-bg-rgb: {{base08-rgb-r}}, {{base08-rgb-g}}, {{base08-rgb-b}} !important;
    --adw-error-fg-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-error-rgb: {{base08-rgb-r}}, {{base08-rgb-g}}, {{base08-rgb-b}} !important;

    /* Window */
    --adw-window-bg-rgb: {{base00-rgb-r}}, {{base00-rgb-g}}, {{base00-rgb-b}} !important;
    --adw-window-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;

    /* Views - e.g. text view or tree view */
    --adw-view-bg-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-view-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;

    /* Header bar, search bar, tab bar */
    --adw-headerbar-bg-rgb: {{base00-rgb-r}}, {{base00-rgb-g}}, {{base00-rgb-b}} !important;
    --adw-headerbar-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;
    --adw-headerbar-border-rgb: {{base02-rgb-r}}, {{base02-rgb-g}}, {{base02-rgb-b}} !important;
    --adw-headerbar-backdrop-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-headerbar-shade-rgb: 0, 0, 0 !important;
    --adw-headerbar-shade-a: 0.36 !important;
    --adw-headerbar-darker-shade-rgb: 0, 0, 0 !important;
    --adw-headerbar-darker-shade-a: 0.9 !important;

    /* Split pane views */
    --adw-sidebar-bg-rgb: {{base00-rgb-r}}, {{base00-rgb-g}}, {{base00-rgb-b}} !important;
    --adw-sidebar-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;
    --adw-sidebar-backdrop-rgb: {{base02-rgb-r}}, {{base02-rgb-g}}, {{base02-rgb-b}} !important;
    --adw-sidebar-shade-rgb: 0, 0, 0 !important;
    --adw-sidebar-shade-a: 0.36 !important;

    --adw-secondary-sidebar-bg-rgb: {{base00-rgb-r}}, {{base00-rgb-g}}, {{base00-rgb-b}} !important;
    --adw-secondary-sidebar-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;
    --adw-secondary-sidebar-backdrop-rgb: {{base02-rgb-r}}, {{base02-rgb-g}}, {{base02-rgb-b}} !important;
    --adw-secondary-sidebar-shade-rgb: 0, 0, 0 !important;
    --adw-secondary-sidebar-shade-a: 0.36 !important;

    /* Cards, boxed lists */
    --adw-card-bg-rgb: 255, 255, 255 !important;
    --adw-card-bg-a: 0.08 !important;
    --adw-card-fg-rgb: 255, 255, 255 !important;
    --adw-card-shade-rgb: 0, 0, 0 !important;
    --adw-card-shade-a: 0.36 !important;

    /* Dialogs */
    --adw-dialog-bg-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-dialog-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;

    /* Popovers */
    --adw-popover-bg-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-popover-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;

    /* Thumbnails */
    --adw-thumbnail-bg-rgb: {{base01-rgb-r}}, {{base01-rgb-g}}, {{base01-rgb-b}} !important;
    --adw-popover-fg-rgb: {{base05-rgb-r}}, {{base05-rgb-g}}, {{base05-rgb-b}} !important;

    /* Miscellaneous */
    --adw-shade-rgb: 0, 0, 0 !important;
    --adw-shade-a: 0.36 !important;
}
